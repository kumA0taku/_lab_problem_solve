using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Compare
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.Write("Please enter 1st number: ");
            int num_1st = int.Parse(Console.ReadLine());
            Console.Write("Please enter 2nd number: ");
            int num_2nd = int.Parse(Console.ReadLine());
            if (num_1st > num_2nd)
            {
                Console.WriteLine("Number1 is more than number2.");
            }
            else
            {
                Console.WriteLine("Number1 is less than number2.");
            }
        }
    }
}

